{
  "name": "myfender/prestashop-laravel",
  "description": "Prestashop Web Service Library for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Ignazio Lo Iacono",
      "email": "loiaging.mt@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "myfender\\PrestashopWebService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "myfender\\PrestashopWebService\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "myfender\\PrestashopWebService\\PrestashopWebServiceProvider"
        ],
        "aliases": {
            "Prestashop": "myfender\\PrestashopWebService\\PrestashopWebServiceFacade"
        }
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "Orchestra/Testbench": "^3.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs src/*"
  }
}
